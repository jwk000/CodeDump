    <Summary>
        <!-- 杂项 称号高度 -->
        <Misc title_height="0.5" />
        <!-- 人气指数=点赞数*n1+收花数*n2 1星线 2星线 3星线 4星线 5星线-->
        <Hot n1="1.6" n2="0.8" star1="10" star2="20" star3="30" star4="40" star5="50" />
        <!-- 资历分=目前角色等级*n1 + 建号天数*n2 + 成就点数*n3 + 紫钻等级*n4-->
        <Career n1="0.1" n2="0.1" n3="0.1" n4="0.1" />
        <!-- 舞技分=事务所总分*n1+三种排位赛历史最高等级分之和*n2-->
        <Dancer n1="0.1" n2="0.1" />
        <!-- 收藏分=图鉴总收集率*n1 + 拥有称号数*n2 + 臻品服装数量*n3 + 轻奢服装数量*n4 + S宠物数量*n5 + A宠物数量*n6-->
        <Collector n1="0.1" n2="0.1" n3="0.1" n4="0.1" n5="0.1" n6="0.1" />
        <!-- 情缘分=历史获得羁绊照片数*n1 + 好友数量*n2 + 被点赞数*n3 + 收花数*n4-->
        <Lover n1="0.1" n2="0.1" n3="0.1" n4="0.1" />
        <Share>
            <Reward type="gb" boy_id="0" girl_id="0" num="1000" />
        </Share>
        <!-- 关键词选取条件
            1 成就点数>=N
            2 三种排位赛历史最高段位都>=某段位
            3 拥有臻品服饰数量>=N
            4 图鉴收集率>=N%"
            5 与羁绊值最高好友的羁绊值>=N
            6 收集图鉴套装>=N
            7 建号天数>=N
            8 累计位赛次数>=N
            9 事务所总分>=N
            10 历史获得SSR照片>=N
            11 称号拥有>=N
            12 拥有宠物>=N
            13 历史获得羁绊旅行照片>=N
            14 宠物旅行藏品收集度>=N%
            15 人气指数>=N
            16 四项总分都>=N
            17 资历分>=N
            18 舞技分>=N
            19 情缘分>=N
            20 收藏分>=N
            0 以上全都不满足
        -->
        <Keyword>
            <!-- 梯队 条件 参数 关键词 -->
            <Key id="1" level="1" condition="1" value="200" desc="成就点数>=N" word="独孤求败" />
            <Key id="2" level="1" condition="2" value="100" desc="三种排位赛历史最高段位都>=某段位" word="神之领域的探索者" />
            <Key id="3" level="1" condition="3" value="100" desc="拥有臻品服饰数量>=N" word="土豪来交个朋友吗" />
            <Key id="4" level="1" condition="4" value="100" desc="图鉴收集率>=N%" word="给大佬递茶" />
            <Key id="5" level="1" condition="5" value="100" desc="与羁绊值最高好友的羁绊值>=N" word="人人都爱本可爱" />
            <Key id="6" level="2" condition="6" value="100" desc="收集图鉴套装>=N" word="可爱舞者，在线换装" />
            <Key id="7" level="2" condition="7" value="100" desc="建号天数>=N" word="炫舞忠实粉丝" />
            <Key id="8" level="2" condition="8" value="100" desc="累计位赛次数>=N" word="输赢乃身外之事" />
            <Key id="9" level="2" condition="9" value="100" desc="事务所总分>=N" word="勤奋的舞者" />
            <Key id="10" level="2" condition="10" value="100" desc="历史获得SSR照片>=N" word="人形锦鲤本鲤" />
            <Key id="11" level="3" condition="11" value="50" desc="称号拥有>=N" word="千面达人" />
            <Key id="12" level="3" condition="12" value="20" desc="拥有宠物>=N" word="动物园园长" />
            <Key id="13" level="3" condition="13" value="100" desc="历史获得羁绊旅行照片>=N" word="爱拍达人" />
            <Key id="14" level="3" condition="14" value="100" desc="宠物旅行藏品收集度>=N%" word="所思在远道" />
            <Key id="15" level="3" condition="15" value="100" desc="人气指数>=N" word="夜空中最亮的星" />
            <Key id="16" level="4" condition="16" value="100" desc="四项总分都>=N" word="无敌是多么的寂寞" />
            <Key id="17" level="5" condition="17" value="100" desc="资历分>=N" word="知名炫舞社会人" />
            <Key id="18" level="5" condition="18" value="100" desc="舞技分>=N" word="舞蹈担当本担" />
            <Key id="19" level="5" condition="19" value="100" desc="情缘分>=N" word="人气idol" />
            <Key id="20" level="5" condition="20" value="100" desc="收藏分>=N" word="超满足的收藏家" />
            <Key id="21" level="9" condition="0" value="100" desc="以上全都不满足" word="我悄悄地来过，又悄悄地走了" />
        </Keyword>
        <UI>
